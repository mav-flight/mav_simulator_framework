<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>mav_gazebo</name>
  <version>0.0.0</version>
  <description>The package to spawn MAV's URDF models to gazebo</description>
  <maintainer email="4303534+suresh-guttikonda@users.noreply.github.com">Suresh G</maintainer>
  <license>Apache License 2.0</license>

  <author>Fadri Furrer</author>
  <author>Michael Burri</author>
  <author>Mina Kamel</author>
  <author>Janosch Nikolic</author>
  <author>Markus Achtelik</author>
  
  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>

  <depend condition="$ROS_VERSION == 2">gazebo_ros</depend>
  <depend condition="$ROS_VERSION == 2">mav_description</depend>
  <depend condition="$ROS_VERSION == 2">robot_state_publisher</depend>
  <depend condition="$ROS_VERSION == 2">xacro</depend>

  <exec_depend condition="$ROS_VERSION == 2">ros2launch</exec_depend>

  <test_depend condition="$ROS_VERSION == 2">ament_lint_auto</test_depend>
  <test_depend condition="$ROS_VERSION == 2">ament_lint_common</test_depend>

  <export>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
  </export>
</package>
